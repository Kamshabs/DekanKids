    <div class="mainmenu-area">
        <div class="container">
            <div class="row">

                <!--
                    This is Main Navigation which checks if the page is Index Action of the default controller then
                    show the particular lines of bootstrap else show other. This is only applied to the starting tag
                    of div tag. It will only load the either one of these.

                -->
                @if (ViewContext.RouteData.GetRequiredString("action") == "Index")
                {
                <text><div class="col-lg-9 col-lg-offset-3 col-md-9 col-md-offset-3 col-sm-9 col-sm-offset-3 col-xs-12">
                </text>
                }

                else
                {
                <text><div class="col-lg-12 col-md-12 col-sm-12">
                </text>

                }

                <div class="menu-area">
                    <div class="mainmenu">
                        <nav>

                            @{
                                List<DokanApp.Models.Category> category = DokanApp.Models.MyMenu.GetMenus();
                                var plist = category.Where(m => m.ParentID == null).ToList(); // This will list main menu items from the database.

                                if (plist != null && plist.Count > 0)
                                {
                            <ul id="nav">

                                @foreach (var pitem in plist.OrderBy(a => a.OrderNumber)) // Lopping through the menu to generate Primary Menu with proper ordernumber in accending & title in arabic/english text.
                                        {
                                        <li>
                                            @{

                                                <a href="/@CultureInfo.CurrentCulture.TextInfo.CultureName@pitem.Url" title="Description">
                                                    @if (ViewBag.rtl == true)
                                                        { @pitem.TitleAr }
                                                        else
                                                        { @pitem.Title }
                                                </a>
                                            }


                                            @{
                                                        var clist = category.Where(m => m.ParentID == pitem.CategoryID).ToList(); // getting list for Submenu
                                                        string namelang = @CultureInfo.CurrentCulture.Name.ToString();
                                                        var title = "";

                                                <text>
                                                    @if (clist != null && clist.Count > 0)
                                                            {
                                                <ul class="sub-menu">
                                                    @foreach (var citem in clist.OrderBy(a => a.OrderNumber))
                                                                    {
                                                                    <li>

                                                                        @{

                                                                        if (ViewBag.rtl == true)
                                                                        {
                                                                            title = @citem.TitleAr;
                                                                        }
                                                                        else
                                                                        {
                                                                            title = @citem.Title;
                                                                        }
                                                                        }

                                                                        @Html.ActionLink(title, "ProductToCategories" + "/" + citem.CategoryID, "default", null, null)

                                                                    </li>
                                                                    }

                                                </ul>
                                                            }

                                                </text>
                                            }

                                        </li>

                                        }
                            </ul>
                                }
                            }

                        </nav>

                    </div>
                </div>
            </div>
        </div>
    </div>
